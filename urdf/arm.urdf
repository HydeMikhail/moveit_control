<?xml version="1.0"?>
<!-- roslaunch urdf_tutorial display.launch model:='$(find custom_arm)/urdf/arm.urdf' -->
<robot name='robotic_arm'>
    <!--  WORLD LINK  -->
    <link name="world"/>
    <!--  BASE LINK  -->
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder radius="0.3" length="0.05"/>
            </geometry>
            <material name="Black"/>
            <origin xyz="0.0 0.0 0.025" rpy="0.0 0.0 0.0"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.3" length="0.05"/>
            </geometry>
            <origin xyz="0.0 0.0 0.025" rpy="0.0 0.0 0.0"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <origin xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.27" iyy="0.0027" izz="0.005" ixy="0.0" ixz="0.0" iyz="0.0"/>
        </inertial>
    </link>

    <!--  BASE JOINT  -->
    <joint name="base_joint" type="continuous">
        <parent link="world"/>
        <child link="base_link"/>
        <axis xyz="0.0 0.0 1.0"/>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </joint>

    <!--  MAIN ARM LINK  -->
    <link name="main_link">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="1.0"/>
            </geometry>
            <material name="Silver">
                <color rgba="0.75 0.75 0.75 1.0"/>
            </material>
            <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="1.0"/>
            </geometry>
            <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <origin xyz="0.0 0.0 0.5"/>
            <inertia ixx="0.02176" iyy="0.00125" izz="0.02416" ixy="0.0" ixz="0.0" iyz="0.0"/>
        </inertial>
    </link>

    <!--  MAIN ARM JOINT  -->
    <joint name="main_joint" type="continuous">
        <parent link="base_link"/>
        <child link="main_link"/>
        <axis xyz="0.0 1 0.0"/>
        <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
    </joint>

    <!--  SEC ARM LINK  -->
    <link name="sec_link">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.75"/>
            </geometry>
            <material name="Silver">
                <color rgba="0.75 0.75 0.75 1.0"/>
            </material>
            <origin xyz="0.0 0.0 0.375" rpy="0.0 0.0 0.0"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="0.75"/>
            </geometry>
            <origin xyz="0.0 0.0 0.375" rpy="0.0 0.0 0.0"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <origin xyz="0.0 0.0 0.375"/>
            <inertia ixx="0.01396" iyy="0.0015" izz="0.01396" ixy="0.0" ixz="0.0" iyz="0.0"/>
        </inertial>
    </link>

    <!--  SEC ARM JOINT  -->
    <joint name="sec_joint" type="continuous">
        <parent link="main_link"/>
        <child link="sec_link"/>
        <axis xyz="0.0 1 0.0"/>
        <origin xyz="0.0 0.0 1" rpy="0.0 -2 0.0"/>
        
    </joint>

    <!--  TOOL ARM LINK  -->
    <link name="tool_link">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.325"/>
            </geometry>
            <material name="Silver">
                <color rgba="0.75 0.75 0.75 1.0"/>
            </material>
            <origin xyz="0.0 0.0 0.1625" rpy="0.0 0.0 0.0"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.05" length="0.325"/>
            </geometry>
            <origin xyz="0.0 0.0 0.1625" rpy="0.0 0.0 0.0"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <origin xyz="0.0 0.0 0.1625"/>
            <inertia ixx="0.01396" iyy="0.0015" izz="0.01396" ixy="0.0" ixz="0.0" iyz="0.0"/>
        </inertial>
    </link>

    <!--  TOOL ARM JOINT  -->
    <joint name="tool_joint" type="continuous">
        <parent link="sec_link"/>
        <child link="tool_link"/>
        <axis xyz="0.0 1 0.0"/>
        <origin xyz="0.0 0.0 0.75" rpy="0.0 1.8504 0.0"/>
    </joint>

   <!--  TRANSMISSION  -->

    <!--  BASE ACTUATOR  -->
    <transmission name="base_trans">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="base_joint">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="base_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <!--  MAIN ACTUATOR  -->
    <transmission name="main_trans">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="main_joint">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="main_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <!--  SECOND ACTUATOR  -->
    <transmission name="sec_trans">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="sec_joint">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="sec_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <!--  TOOL ACTUATOR  -->
    <transmission name="tool_trans">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="tool_joint">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="tool_motor">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <gazebo>
        <plugin name="control" filename="libgazebo_ros_control.so">
            <robotNamespace>/arm</robotNamespace>
        </plugin>
    </gazebo>

    <gazebo>
        <plugin name="joint_state_publisher" filename="libgazebo_ros_joint_state_publisher.so">
            <joint_name>base_joint,main_joint,sec_joint,tool_joint</joint_name>
        </plugin>
    </gazebo>

</robot>